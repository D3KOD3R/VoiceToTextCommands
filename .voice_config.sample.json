{
  "repos": {
    "local": {
      "path": ".",
      "issuesFile": "voiceissues/voice-issues.md"
    }
  },
  "defaultRepo": "local",
  "hotkeys": {
    "toggle": "ctrl+alt+i",
    "quit": "ctrl+alt+q"
  },
  "devices": {
    "allowlist": [],
    "denylist": [],
    "lastSelected": ""
  },
  "phrases": {
    "nextIssue": ["next issue", "next point"],
    "stop": ["end issues", "stop issues"]
  },
  "stt": {
    "provider": "whisper_cpp",
    "binaryPath": ".tools/whisper/whisper-cli.exe",
    "model": ".tools/whisper/ggml-base.en.bin",
    "language": "en",
    "inputSamplerate": null,
    "inputChannels": null
  },
  "realtime": {
    "wsUrl": "ws://localhost:8000/ws",
    "postUrl": "http://localhost:8000/transcript"
  }
}
